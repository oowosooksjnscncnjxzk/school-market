.tab-bar {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 60px;
  display: flex;
  background-color:#f5f5f5;
  border-top: 1px solid #eee;
}

.tab-bar-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.tab-bar-icon {
  width: 40px;
  height: 40px;
}

.tab-bar-text {
  font-size: 12px;
}



