.container{
  padding: 20rpx 0rpx;
  background-color: #f5f5f5;
}

.search-bar {
  display: flex;
  align-items: center;
  padding: 10px;
  background-color: #ffffff;
  border-radius: 20px;
  margin-bottom: 10px;
  width: 310px;
  height: 30rpx;
  border: 3rpx solid #f3eeee;
}

.search-bar input {
  flex: 1;
  border: none;
  outline: none;
  background: transparent;
  margin-left: 10px;
}

.search-icon {
  width: 20px;
  height: 20px;
}
.add-icon{
  position: fixed;
  height: 100rpx;
  width: 100rpx;
  right:20px;
  bottom:90px;
}


